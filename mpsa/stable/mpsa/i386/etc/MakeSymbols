TCL_VER = 8.0
TCL_LIB_DIR = /usr/lib
TCL_INCLUDE_DIR =/usr/include
TCL_LIB = -ltcl$(TCL_VER)
PLATFORM = i386
MPSA_LIB = -lm -lc $(TCL_LIB)
MPSA_INCLUDE = -I$(TCL_INCLUDE_DIR)
MPSA_SRC_DIR = /home/gw219/MPSA/stable/mpsa/$(PLATFORM)/src
MPSA_COMPILER_FLAGS = -O2 -fPIC -DMPSA_ETC_DIR=\"$(MPSA_SRC_DIR)/../etc\"
MPSA_SHARED_FLAGS = -shared
MPSA_LINKER_FLAGS = $(MPSA_SHARED_FLAGS) $(MPSA_COMPILER_FLAGS)


